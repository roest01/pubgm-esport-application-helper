{
  "name": "pubgm-esport-application-helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created to help creating the schedule for pubg mobile tournaments",
  "homepage_url": "https://euroelite.online",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "https://esports.pubgmobile.com/3rdparty/*",
    "https://esports.pubgmobile.com/*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://esports.pubgmobile.com/3rdparty/*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.js"
      ]
    }
  ]
}
